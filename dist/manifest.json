{
    "manifest_version": 2,
    "name": "Geoguessr Party",
    "version": "1.0",
    "description": "1, 2, 3 e vai!",
    "browser_action": {
        "default_icon": "icon-16.png"
    },
    "background": {
        "scripts": ["js/backgroundPage.js"],
        "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["js/content.js"]
      }
    ],
    "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png"
    },
    "permissions": ["tabs", "activeTab", "notifications", "http://*/", "https://*/"]
}
